{
	"name": "ecommerce-site",
	"private": true,
	"version": "0.1.0",
	"description": "Ecommerce site",
	"type": "module",
	"packageManager": "pnpm@6.23.5+",
	"main": "src/index.js",
	"scripts": {
		"start": "node src/index.js",
		"format": "prettier --check \"**/*.{js,jsx,css,md,mdx,yaml,yml,toml}\"",
		"format:fix": "prettier --write \"**/*.{js,jsx,css,md,mdx,yaml,yml,toml}\"",
		"lint": "run-s -c lintcss lintjs",
		"lint:fix": "run-s -c lintcss:fix lintjs:fix",
		"lintcss": "stylelint \"**/*.css\"",
		"lintcss:fix": "stylelint \"**/*.css\" --fix",
		"lintjs": "eslint .",
		"lintjs:fix": "eslint --fix .",
		"typecheck": "tsc"
	},
	"engines": {
		"node": ">=16.10.0"
	},
	"homepage": "https://github.com/owner/project#README.md",
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/project/repo"
	},
	"bugs": {
		"url": "https://github.com/owner/repo/issues"
	},
	"contributors": [
		{
			"name": "Edwin Kofler",
			"email": "edwin@kofler.dev",
			"url": "https://edwinkofler.com"
		}
	],
	"keywords": [
		"website",
		"ecommerce"
	],
	"dependencies": {
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"typescript": "^4.5.2"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/express": "^4.17.13",
		"@types/node": "^16.11.11",
		"eslint": "^8.3.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-prettier": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.0",
		"stylelint": "^14.1.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recommended": "^6.0.0",
		"stylelint-config-standard": "^24.0.0",
		"stylelint-prettier": "^2.0.0"
	}
}
